namespace FExist

kinds:
  var "x" => Exp
  tvar "X" => Ty

inductive Ty : Sig -> Type
| tvar : BVar s .tvar -> Ty s
| arrow : Ty s -> Ty s -> Ty s
| forall_ : Ty (s,X) -> Ty s
| exists_ : Ty (s,X) -> Ty s
| prod : Ty s -> Ty s -> Ty s
| unit : Ty s

inductive Exp : Sig -> Type
| var : BVar s .var -> Exp s
| abs : Ty s -> Exp (s,x) -> Exp s
| app : Exp s -> Exp s -> Exp s
| tabs : Exp (s,X) -> Exp s
| tapp : Exp s -> Ty s -> Exp s
| pack : Ty s -> Exp s -> Ty (s,X) -> Exp s
| unpack : Exp s -> Exp ((s,X),x) -> Exp s
| pair : Exp s -> Exp s -> Exp s
| fst : Exp s -> Exp s
| snd : Exp s -> Exp s
| unit : Exp s
